{
  "name": "tiny-sdk",
  "version": "0.0.0",
  "description": "A sdk of tiny tools.",
  "author": "Chris <https://github.com/zyyv>",
  "license": "MIT",
  "homepage": "https://github.com/zyyv/starter-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyyv/starter-ts.git"
  },
  "bugs": {
    "url": "https://github.com/zyyv/starter-ts/issues"
  },
  "keywords": [
    "sdk",
    "tiny",
    "tools"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "lint": "eslint .",
    "test": "vitest",
    "test:update": "vitest -u",
    "typecheck": "tcs --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@babel/types": "^7.20.2",
    "@types/node": "^18.11.9",
    "bumpp": "^8.2.1",
    "eslint": "^8.27.0",
    "esno": "^0.16.3",
    "rimraf": "^4.1.2",
    "typescript": "^4.8.4",
    "unbuild": "^1.1.1",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  }
}
