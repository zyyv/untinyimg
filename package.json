{
  "name": "untinyimg",
  "type": "module",
  "version": "0.0.2",
  "description": "A cli tools of tinypng.",
  "author": "Chris <https://github.com/zyyv>",
  "license": "MIT",
  "homepage": "https://github.com/zyyv/untinyimg#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyyv/untinyimg.git"
  },
  "bugs": {
    "url": "https://github.com/zyyv/untinyimg/issues"
  },
  "keywords": [
    "untiny",
    "tinypng",
    "tools"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "untiny": "./bin/untiny.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint --cache .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:update": "vitest -u",
    "typecheck": "tcs --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "consola": "^2.15.3",
    "picocolors": "^1.0.0",
    "tinify": "^1.7.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@babel/types": "^7.21.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "rimraf": "^4.4.0",
    "typescript": "^4.8.4",
    "unbuild": "^1.1.2",
    "unconfig": "^0.3.7",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  }
}
